  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Ingest Events Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(ingest_events_total[5m])) by (ingest_type)",
          "legendFormat": "{{ingest_type}}"
        }
      ],
      "options": {
        "legend": { "displayMode": "list" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "P95 Ingest Latency",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(ingest_latency_ms_bucket[5m])) by (le, ingest_type))",
          "legendFormat": "{{ingest_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ms" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["observability", "ingest"],
